<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER_LEFT" prefHeight="28.0" prefWidth="762.0" spacing="10.0" style="-fx-background-color: #988A84;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxml.ItemContentController">
   <children>
      <Label fx:id="itemId" alignment="CENTER" prefHeight="25.0" prefWidth="24.0" text="ID" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="itemNama" alignment="CENTER" contentDisplay="CENTER" prefHeight="25.0" prefWidth="130.0" text="Nama" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="itemLokasi" alignment="CENTER" prefHeight="25.0" prefWidth="130.0" text="Lokasi" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Region prefHeight="28.0" prefWidth="69.0" HBox.hgrow="ALWAYS" />
      <Label fx:id="itemStatus" alignment="CENTER" prefHeight="25.0" prefWidth="88.0" text="Status" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <HBox alignment="CENTER" prefHeight="25.0" prefWidth="90.0">
         <children>
            <Button mnemonicParsing="false" onAction="#selectItem" style="-fx-background-color: #F45B69;">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../Symbols/line/trash.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
