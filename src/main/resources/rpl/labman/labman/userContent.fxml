<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER_LEFT" prefHeight="28.0" prefWidth="762.0" spacing="10.0" style="-fx-background-color: #988A84;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxml.UserContentController">
   <children>
      <Label fx:id="userEmail" alignment="CENTER" prefHeight="25.0" prefWidth="225.0" text="Email" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="userName" alignment="CENTER" contentDisplay="CENTER" prefHeight="25.0" prefWidth="130.0" text="Username" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="userPass" alignment="CENTER" prefHeight="25.0" prefWidth="130.0" text="Password" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="userRole" alignment="CENTER" prefHeight="25.0" prefWidth="152.0" text="Role" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <HBox alignment="CENTER" maxWidth="120.0" prefHeight="28.0" prefWidth="72.0" spacing="10.0">
         <children>
            <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteUser" style="-fx-background-color: #6BA292;">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="23.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../Symbols/line/edit-black-point.png" />
                     </image>
                  </ImageView>
               </graphic>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
